{
  "name": "dashboard-layout",
  "version": "1.0.0",
  "description": "dashboard layout",
  "homepage": "https://amoebiusss.github.io/learning-app-layout",
  "scripts": {
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run stylelint",
    "build": "gulp build",
    "start": "gulp start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "autoprefixer": "9.5.x",
    "browser-sync": "2.26.x",
    "del": "^4.1.1",
    "editorconfig": "^0.15.3",
    "gulp": "4.0.x",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^6.0.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "posthtml-include": "^1.2.0",
    "stylelint": "9.10.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
